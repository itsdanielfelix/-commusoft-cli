{
    "name": "itsdanielfelix/commusoft-cli",
    "description": "Cli tool commusoft.",
    "type": "project",
    "authors": [
        {
            "role": "Developer",
            "name": "Daniel Felix",
            "email": "danielfelix1995@gmail.com",
            "homepage": "https://danielfelix.in/"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.0",
        "laravel-zero/framework": "^9.1",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.5",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["commusoft"]
}
