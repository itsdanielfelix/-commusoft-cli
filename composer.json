{
    "name": "itsdanielfelix/commusoft-cli",
    "description": "Cli tool commusoft.",
    "type": "project",
    "authors": [
        {
            "role": "Developer",
            "name": "Daniel Felix",
            "email": "danielfelix1995@gmail.com",
            "homepage": "https://danielfelix.in/"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.0",
        "laravel-zero/framework": "^10.2",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.5",
        "larastan/larastan": "^2.0",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "build": "php commusoft app:build"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/commusoft"]
}
